        <dc:title>His Majesty's Dragon</dc:title>
        <dc:creator opf:file-as="Novik, Naomi" opf:role="aut">Naomi Novik</dc:creator>
        <dc:contributor opf:file-as="calibre" opf:role="bkp">calibre (0.7.35) [http://calibre-ebook.com]</dc:contributor>
